{
  "name": "react-template-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:mock": "cross-env useMock=true appEnv=mock vite",
    "dev:test": "cross-env appEnv=test vite",
    "build": "tsc && cross-env appEnv=production vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{less,postcss,css,scss}\" --custom-syntax postcss-less --cache --cache-location node_modules/.cache/stylelint/",
    "format": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "prepare": "husky install",
    "server": "serve ./dist -p 52232"
  },
  "dependencies": {
    "antd": "^4.23.6",
    "axios": "^1.3.5",
    "events": "^3.3.0",
    "intersection-observer": "^0.12.2",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@loadable/component": "^5.15.3",
    "@reduxjs/toolkit": "^1.9.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.192",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^3.1.0",
    "ahooks": "^3.7.6",
    "classnames": "^2.3.2",
    "consola": "^3.0.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "mockjs": "^1.1.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.7",
    "qs": "^6.11.1",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "rimraf": "^5.0.0",
    "stylelint": "^15.4.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-less": "^1.0.6",
    "typescript": "^4.6.3",
    "vite": "^4.2.1",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-style-import": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --loglevel warn"
    ],
    "src/**/*.{less,postcss,css,scss}": [
      "stylelint --fix --custom-syntax postcss-less --cache --cache-location node_modules/.cache/stylelint/"
    ]
  }
}
