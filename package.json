{
  "name": "react-template-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:mock": "cross-env useMock=true appEnv=mock vite",
    "dev:test": "cross-env appEnv=test vite",
    "build": "tsc && cross-env appEnv=production vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{less,postcss,css,scss}\" --custom-syntax postcss-less --cache --cache-location node_modules/.cache/stylelint/",
    "format": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "prepare": "husky install",
    "server": "serve ./dist -p 52232"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@loadable/component": "^5.16.7",
    "@reduxjs/toolkit": "^2.8.1",
    "@wangeditor-next/editor": "^5.6.34",
    "@wangeditor-next/editor-for-react": "^1.0.9",
    "ahooks": "^3.8.4",
    "antd": "^5.4.2",
    "axios": "^1.9.0",
    "classnames": "^2.3.2",
    "consola": "^3.4.2",
    "dayjs": "^2.0.0-alpha.4",
    "echarts": "^5.4.3",
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "qs": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.4",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.0",
    "react-router-dom": "^7.6.0",
    "redux": "^5.0.1",
    "redux-logger": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.196",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.25",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-legacy": "^6.1.1",
    "@vitejs/plugin-react": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.3",
    "mockjs": "^1.1.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.27",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "stylelint": "^16.19.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.1.6",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-style-import": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --loglevel warn"
    ]
  }
}
