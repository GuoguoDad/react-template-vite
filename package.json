{
  "name": "react-template-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:mock": "cross-env useMock=true appEnv=mock vite",
    "dev:test": "cross-env appEnv=test vite",
    "build": "tsc && cross-env appEnv=production vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{less,postcss,css,scss}\" --custom-syntax postcss-less --cache --cache-location node_modules/.cache/stylelint/",
    "format": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "prepare": "husky install",
    "server": "serve ./dist -p 52232"
  },
  "dependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@loadable/component": "^5.15.2",
    "@reduxjs/toolkit": "^1.8.3",
    "ahooks": "^3.5.2",
    "antd": "^4.23.6",
    "axios": "^1.1.3",
    "classnames": "^2.3.1",
    "consola": "^2.15.3",
    "dayjs": "^1.11.3",
    "echarts": "^5.3.3",
    "events": "^3.3.0",
    "intersection-observer": "^0.12.2",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.24",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-react": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.14",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-less": "^1.0.6",
    "terser": "^5.14.1",
    "typescript": "^4.6.3",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.2",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-progress": "^0.0.6",
    "vite-plugin-style-import": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --loglevel warn"
    ],
    "src/**/*.{less,postcss,css,scss}": [
      "stylelint --fix --custom-syntax postcss-less --cache --cache-location node_modules/.cache/stylelint/"
    ]
  }
}
